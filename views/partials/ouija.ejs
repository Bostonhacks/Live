<canvas id="ouijaBoard" class="canvas"></canvas>

<script>

var canvas = document.getElementById("ouijaBoard");
var context = canvas.getContext("2d");
canvas.width = 800;
canvas.height = 532.5;

var background = new Image();
background.src = "img/board.png";
marker = new Image();
marker.src = "img/marker.png";

background.onload = function(){
	context.drawImage(background,0,0,background.width,background.height,0,0,canvas.width,canvas.height);   
}	
context.drawImage(marker, 50, 50);

</script>

